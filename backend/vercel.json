{
    "builds": [
        {
            "src": "backend/app.js",
            "use": "@vercel/node",
            "config": {
                "includeFiles": [
                    "backend/swagger.yaml"
                ]
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "backend/app.js"
        }
    ]
}